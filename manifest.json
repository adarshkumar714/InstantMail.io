{
    "manifest_version": 3,
    "name": "InstantMail.io",
    "version": "1.0",
    "description": "Mail anything instantly.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab"
    ],
    "icons": {
        "256": "logos/logo.png"
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "content.js"
        ]
      }
    ]
  }